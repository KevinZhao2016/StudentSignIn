<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.model.AdminEntity" table="Admin" schema="StuDB">
        <id name="aid">
            <column name="AID" sql-type="int(11)"/>
        </id>
        <property name="aUserName">
            <column name="AUserName" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="aPassword">
            <column name="APassword" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>